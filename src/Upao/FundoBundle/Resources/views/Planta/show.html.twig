{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Planta</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Columna</th>
                <td>{{ entity.columna }}</td>
            </tr>
            <tr>
                <th>Fila</th>
                <td>{{ entity.fila }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('planta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('planta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
