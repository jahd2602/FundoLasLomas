{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Fumigacion</h1>

    <table class="record_properties table-striped">
        <tbody>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ entity.observacion }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>


    <a class="btn btn-primary" href="{{ path('fumigacion') }}">
        Regresar a la lista
    </a>
    <div class="clearfix"></div>


    {{ render(controller('UpaoFundoBundle:Fundo:muestra', { 'plantas': plantas })) }}



{% endblock %}
