<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Upao\FundoBundle\Entity\Cosecha" table="cosecha">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="kilosPrimera" type="float" column="kilos_primera" nullable="false"/>
        <field name="kilosSegunda" type="float" column="kilos_segunda" nullable="false"/>
        <field name="kilosDescarte" type="float" column="kilos_descarte" nullable="false"/>
        <field name="fecha" type="datetime" column="fecha" nullable="true"/>
        <field name="observaciones" type="text" column="observaciones" nullable="true"/>
    </entity>
</doctrine-mapping>
