<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Upao\FundoBundle\Entity\Proveedor" table="proveedor">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="250" nullable="false"/>
    <field name="direccion" type="string" column="direccion" length="250" nullable="true"/>
    <field name="telefono" type="string" column="telefono" length="12" nullable="true"/>
    <field name="ruc" type="string" column="ruc" length="11" nullable="false"/>
    <field name="contacto" type="string" column="contacto" length="250" nullable="true"/>
  </entity>
</doctrine-mapping>
